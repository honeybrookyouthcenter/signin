<Application x:Class="HBCCSignIn.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:controls="using:Microsoft.UI.Xaml.Controls"
             RequestedTheme="Light">
    <Application.Resources>
        <controls:XamlControlsResources>
            <controls:XamlControlsResources.MergedDictionaries>
                <ResourceDictionary>
                    <x:String x:Key="SignInIcon"></x:String>
                    <SolidColorBrush x:Key="SignInForeground" Color="Green" />

                    <x:String x:Key="SignOutIcon"></x:String>

                    <SolidColorBrush x:Key="CancelForeground" Color="Firebrick" />
                </ResourceDictionary>
            </controls:XamlControlsResources.MergedDictionaries>
        </controls:XamlControlsResources>
    </Application.Resources>
</Application>
